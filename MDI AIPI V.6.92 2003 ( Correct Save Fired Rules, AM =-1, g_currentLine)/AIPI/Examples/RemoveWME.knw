RULE "Removing W5"(WEIGHT : 2)
{
IF		$x$		on		$y$
AND		$y$		left_of	$z$
AND		$z$		color		"red"
AND		$z$		on		$w$
AND		$w$		on		"table"
THEN	block5	right_of	"block4"
AND     RETRACT( block2  	left_of    "block3") 
}

RULE "Adding W5"(WEIGHT : 1)
{

IF         block1      on           "block3"
AND     block3	   left_of	"block4"
//THEN    ASSERT( block2		left_of     "block3")	
THEN	block2	 left_of    "block3"
}



